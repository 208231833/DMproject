            <span class="bold">个人搜索器</span><span>--用于保存搜索条件，方便下次选择相同的搜索条件</span>
        </div>
        <div id="conditionDiv">
        </div>
        <br />
        <br />
        <hr />
        <div class="text-center">
            搜索器名称:<input type="text" name="conditionFlagName2" title="搜索器名称" />
            <input type="button" value="保存搜索条件" onclick='saveCondition($("input[name=\"conditionFlagName2\"]").val())'
                class="w80 button button-a button-a-blue" />
            <input type="button" value="关闭" onclick="$('#floatDiv').hide(); return false;" class="w80 button button-a " />
            <br />
            <br />
        </div>
    </div>
    
    <script type="text/javascript">
        $(function () {
            

            $('#salaryMax,#salaryMin').blur(function () {
                if ($(this).val() != '') $(this).parentsUntil('dl').find(":button").trigger('click');
            });

            $('#salaryMinButton,#salaryMaxButton').click(function () {
                var salary = $(this).parentsUntil('dl').find(":text").val();

                if (!/^[0-9]+$/.test(salary)) {
                    alert("请输入数字!");
                    return;
                }

                var $dl = $(this).parentsUntil('dl');
                var $a = $dl.find("a[data-value='" + salary + "']");



                if ($a.length == 0) {
                    $dl.find("a[data-created='d']").remove();

                    $dl.find(".selected").removeClass('selected');
                    $a = $('<a data-created="d" data-value="' + salary + '">' + salary + '</a>').addClass('selected');
                    $dl.find("a[data-value]:last").after($a);

                } else {
                    //                    $dl.find(".selected").removeClass('selected');
                    //                    $a.addClass('selected');
                    $a.trigger("click");
                }
            });


            setMapText();
        });
    </script>

            
