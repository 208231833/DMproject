    <script type="text/javascript">
        if (getCookie("cookie_RecruitSearch_AutoClose") == "false") {
            $('#autoClose').prop('checked', false);
        } else {
            $('#autoClose').prop('checked', true);
        }
        if ($('#autoClose').prop('checked')) {
            var gets = urlGet();
            var value = getQueryString('g', gets);
            if (value == 'g') {
                $('#moreCondition').hide();
                $('#showMore').removeClass('showMoreClose');
            } else {
                $('#moreCondition').show();
                $('#showMore').addClass('showMoreClose');
            }

        } else {
            if (getCookie("cookie_RecruitSearch_ShowMore") == "true") {
                $('#moreCondition').show();
                $('#showMore').addClass('showMoreClose');
            } else {
                $('#moreCondition').hide();
                $('#showMore').removeClass('showMoreClose');
                //$('#showMore').text('更多搜索条件↓');
            }
        }
    </script>
